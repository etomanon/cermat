{
  "name": "cermat",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "db:setup": "npm-run-all -s db:migrate db:seed-prod csv",
    "start": "ts-node-dev --respawn --no-notify --transpile-only ./src/app.ts",
    "lint:es": "eslint ./src/**/*.ts --fix",
    "db:migrate": "knex migrate:latest",
    "db:seed-test": "ts-node-dev --no-notify ./src/seed/test/index.ts",
    "db:seed-prod": "ts-node-dev --no-notify ./src/seed/prod/index.ts",
    "csv": "ts-node-dev --no-notify ./src/scripts/csv/csv-generate.ts"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/fs-extra": "^9.0.5",
    "@types/geojson": "^7946.0.7",
    "@types/json2csv": "^5.0.1",
    "@types/lodash": "^4.14.165",
    "@types/passport": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fs-extra": "^9.0.1",
    "helmet": "^4.2.0",
    "json2csv": "^5.0.5",
    "knex": "^0.21.13",
    "lodash": "^4.17.20",
    "nocache": "^2.1.0",
    "objection": "^2.2.3",
    "pg": "^8.5.1"
  }
}
